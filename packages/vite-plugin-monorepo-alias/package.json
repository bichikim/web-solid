{
  "name": "@winter-love/vite-plugin-monorepo-alias",
  "version": "1.0.37",
  "description": "vite monorepo alias plugin",
  "keywords": [
    "vitest",
    "storybook-vite",
    "vite",
    "monorepo",
    "alias"
  ],
  "repository": "git@github.com:winter-love/web.git",
  "license": "MIT",
  "author": "bichi@live.co.kr",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": "./*",
    "./plugin": {
      "import": "./dist/plugin.mjs",
      "require": "./dist/plugin.js"
    }
  },
  "main": "dist/index.js",
  "jsdelivr": "dist/index.global.js",
  "unpkg": "dist/index.global.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*",
    "*.d.ts"
  ],
  "scripts": {
    "build": "vite build",
    "check-types": "tsc --noEmit",
    "dev": "vite build --watch --minify false",
    "lint": "cross-env NODE_ENV=script eslint src",
    "lint:fix": "cross-env NODE_ENV=script eslint src --fix",
    "preview": "vite build -c vite.preview.config.mts"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@winter-love/vite-config": "workspace:*"
  },
  "peerDependencies": {
    "vite": "^6"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "d2b83a5ae925fd122a6f46c99d743ede11b86c2f",
  "stage": "alpha",
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/plugin.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "dist",
    "sourcemap": true,
    "splitting": false
  }
}
