{
  "name": "@winter-love/web-solid",
  "version": "0.0.1",
  "private": true,
  "description": "coong web & app service",
  "author": "Bichi Kim <bichi@live.co.kr>",
  "imports": {
    "#*": [
      "./*",
      "./*.ts",
      "./*.tsx"
    ]
  },
  "workspaces": [
    "./packages/*",
    "./apps/*"
  ],
  "scripts": {
    "format:package.json": "pnpm dlx sort-package-json \"package.json\" \"packages/*/package.json\" \"apps/*/package.json\" ",
    "postinstall": "pnpm run format:package.json",
    "lint": "pnpm run -r --filter='@winter-love/*' lint",
    "prepare": "pnpm run -r --filter='@winter-love/*' build",
    "start:local-docker": "docker-compose up -d",
    "storybook": "storybook dev -p 6006",
    "test": "vitest"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@solidjs/testing-library": "^0.8.10",
    "@storybook/addon-a11y": "^10.1.0",
    "@storybook/addon-docs": "^10.1.0",
    "@storybook/addon-links": "^10.1.0",
    "@storybook/addon-onboarding": "^10.1.0",
    "@storybook/addon-vitest": "^10.1.0",
    "@storybook/builder-vite": "^10.1.0",
    "@storybook/test": "^8.6.14",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitest/browser": "^4.0.14",
    "@vitest/browser-playwright": "^4.0.14",
    "@vitest/coverage-v8": "^4.0.14",
    "@winter-love/eslint-config": "workspace:*",
    "@winter-love/test-utils": "workspace:*",
    "@winter-love/vite-config": "workspace:*",
    "@winter-love/vite-plugin-monorepo-alias": "^1.0.37",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^3.3.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "flush-promises": "^1.0.2",
    "playwright": "^1.57.0",
    "prettier": "^3.6.2",
    "reflect-metadata": "^0.2.2",
    "solid-js": "^1.9.10",
    "storybook": "^10.1.0",
    "storybook-solidjs-vite": "^10.0.9",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-solid": "^2.11.10",
    "vitest": "^4.0.14"
  },
  "packageManager": "pnpm@10.23.0+sha512.21c4e5698002ade97e4efe8b8b4a89a8de3c85a37919f957e7a0f30f38fbc5bbdd05980ffe29179b2fb6e6e691242e098d945d1601772cad0fef5fb6411e2a4b",
  "engines": {
    "node": ">= 16",
    "pnpm": ">= 10.10.0"
  },
  "pnpm": {
    "overrides": {
      "ansi-regex@<5.0.1": "5.0.1",
      "browserslist@<4.16.5": "4.16.5",
      "chokidar@<3.5.3": "3.5.3",
      "esbuild@<0.13.14": "0.13.14",
      "follow-redirects@<1.14.7": "1.14.7",
      "glob-parent@<5.1.2": "5.1.2",
      "immer@<9.0.6": "9.0.6",
      "node-fetch@<2.6.7": "2.6.7",
      "prismjs@<1.27.0": "1.27.0",
      "set-value@<4.0.1": "4.0.1",
      "simple-get@<2.8.2": "2.8.2",
      "trim@<0.0.3": "0.0.3",
      "uuid@<8.3.2": "8.3.2"
    }
  },
  "productName": "Coong"
}
